<h1>List of all actors</h1>
<hr>

<table border="1">
  <tbody>
    <% @actors.map{|actor|%>
      <tr>
        <td><%= actor.id %></td>
        <td><img src=<%actor.image%> /></td>
        <td><%= actor.name %></td>
        <td><%= actor.dob%></td>
        <td><a href="/actors/<% actor.id %>">Show details</a><td>
      </tr>
    <% } %>
  </tbody>
</table>
<%= @actors %>
